<struts>

	
	
	<package name="sizeAction" namespace="/" extends="base">
		
		<action name="size_*" class="sizeAction" method="{1}">
			<result name="list">/WEB-INF/jsp/SizeAction/list.jsp</result>
			<result name="toList" type="redirectAction">size_list</result>
		</action>
		
	</package>




</struts>
