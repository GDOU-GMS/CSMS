<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	
	
	<package name="assistAction" namespace="/" extends="base">
		
		<action name="user_updatePwd" class="assistAction" method="updatePwd">
			<result name="updatePwd">/WEB-INF/jsp/AssistAction/updatePwd.jsp</result>
	 	</action>
	 	
	 	<action name="user_renewPwd" class="assistAction" method="renewPwd">
			<result name="success">/WEB-INF/jsp/AssistAction/message.jsp</result>
		    <result name="error">/WEB-INF/jsp/AssistAction/error.jsp</result> 
	 	</action>
		
	</package>




</struts>
